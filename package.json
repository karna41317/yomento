{
  "name": "yemonto",
  "author": "Karan Reddy",
  "description": "Yemo app for leadership coaching",
  "license": "MIT",
  "homepage": "http://yemonto.com",
  "bugs": "https://github.com/karna41317/yemonto.git/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/karna41317/yemonto.git"
  },
  "keywords": [
    "React Native",
    "Starter Kit",
    "Bitrise",
    "Redux",
    "Auth0"
  ],
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "bundle:ios": "node ./node_modules/react-native/local-cli/cli.js bundle --platform ios --entry-file index.ios.js --bundle-output ios/yemonto/main.jsbundle --dev=false --verbose",
    "test:watch": "jest --watch",
    "lint": "eslint src test",
    "coverage": "rimraf coverage && jest --coverage",
    "version": "support/version-ios.sh",
    "assembleDebug": "npm run _unlink > /dev/null 2>&1; npm run _resetPacker && npm run _assembleDebug && npm run _patchDebug && npm run _assembleDebug",
    "assembleRelease": "npm run _unlink > /dev/null 2>&1; npm run _resetPacker && cd ./android && ./gradlew assembleRelease && cd ../",
    "codePushSilent": "code-push release RNAirbnb patch '<0.1.1' -d Production --mandatory --des '{\"mandatoryInstallMode\": 1}'",
    "eslint": "node ./node_modules/eslint/bin/eslint.js src/** libs/*/src/**",
    "patch": "rm -rf patch && mkdir -p patch && react-native bundle --platform android --dev false --entry-file index.android.js --bundle-output patch/index.android.bundle --assets-dest patch/",
    "reset": "watchman watch-del-all && rm -rf ./node_modules && rm -rf ./package-lock.json && rm -rf $TMPDIR/react-* && npm install",
    "start": "node node_modules/react-native/local-cli/cli.js start --reset-cache",
    "stop": "cd ./android && ./gradlew --stop && cd ../",
    "test": "jest",
    "_assembleDebug": "cd ./android && ./gradlew assembleDebug && cd ../",
    "_patchDebug": "react-native bundle --platform android --dev false --entry-file index.android.js --bundle-output android/app/build/intermediates/assets/debug/index.android.bundle --assets-dest android/app/build/intermediates/res/merged/debug/",
    "_resetPacker": "rm -rf react-native-packager-cache* && rm -rf haste-map-react-native-packager*",
    "_unlink": "unlink node_modules/react-native/third-party/glog-0.3.4/test-driver"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-navigation)"
    ]
  },
  "dependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "bluebird": "^3.3.5",
    "event-emitter": "^0.3.4",
    "immutable": "^3.7.6",
    "lodash": "^4.11.0",
    "moment": "^2.12.0",
    "react": "16.0.0-alpha.12",
    "react-dom": "~15.4.2",
    "react-native": "^0.47.2",
    "react-native-code-push": "5.1.3-beta",
    "react-native-lightbox": "^0.7.0",
    "react-native-linear-gradient": "^2.3.0",
    "react-native-linkedin-login": "^1.44.1",
    "react-native-mdcore": "^0.1.9",
    "react-native-navigation-experimental-compat": "^1.1.0",
    "react-native-photo-view": "^1.5.2",
    "react-native-share": "^1.0.23",
    "react-native-svg": "^5.4.2",
    "react-native-transformable-image": "0.0.18",
    "react-native-vector-icons": "^4.4.2",
    "react-navigation": "1.0.0-beta.14",
    "react-redux": "^4.4.5",
    "redux": "^3.4.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "3.0.6",
    "redux-loop-symbol-ponyfill": "^2.2.0",
    "redux-persist": "^4.10.1",
    "redux-persist-migrate": "^5.0.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.0.1",
    "reselect": "^3.0.1",
    "standard-http-error": "^2.0.0",
    "tinycolor2": "^1.4.1"
  },
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-eslint": "^7.1.0",
    "babel-jest": "^17.0.2",
    "babel-polyfill": "^6.9.0",
    "babel-preset-react-native": "^1.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "enzyme": "^2.2.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-react": "^6.7.1",
    "fetch-mock": "^5.5.0",
    "istanbul": "1.0.0-alpha.2",
    "jest": "^17.0.2",
    "plop": "^1.7.4",
    "react-addons-test-utils": "~15.4.2",
    "react-native-mock": "~0.2.5",
    "react-test-renderer": "~15.4.2",
    "remote-redux-devtools": "^0.5.7",
    "rimraf": "^2.5.2"
  }
}
